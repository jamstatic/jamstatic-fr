{
  "name": "jamstatic.fr",
  "version": "0.3.0",
  "description": "Jamstatic website",
  "author": "Frank Taillandier",
  "contributors": [
    {
      "name": "Arnaud Ligny",
      "url": "https://arnaudligny.fr"
    }
  ],
  "homepage": "https://github.com/jamstatic/jamstatic-fr#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamstatic/jamstatic-fr.git"
  },
  "bugs": {
    "url": "https://github.com/jamstatic/jamstatic-fr/issues"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5",
    "@tailwindcss/typography": "^0.5",
    "all-contributors-cli": "^6.26",
    "tailwindcss": "^3.4"
  },
  "scripts": {
    "css:watch": "npx tailwindcss -i ./assets/css/tailwind.css -o ./assets/css/styles.css --watch",
    "css:build": "npx tailwindcss -i ./assets/css/tailwind.css -o ./assets/css/styles.css --minify",
    "build": "curl -sSOL https://cecil.app/build.sh && bash ./build.sh",
    "serve": "cecil serve -vv --config=config/dev.yml",
    "contributors:add": "all-contributors add",
    "contributors:check": "all-contributors check",
    "contributors:generate": "all-contributors generate"
  }
}
